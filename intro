#!/bin/bash
#this is toodoo intro version < 1
#it is evaluation, examination, experiment and exercise
#the code is commented in english

#retrieve name and arguments
self[0]=`basename "$0"`;
self[1]=$1;
self[2]=$2;
self[3]=$3;
self[4]=$4;
self[5]=$5;
#output arguments
for i in ${self[@]}; do echo $i; done

#the arguments array is passed to var1 to var5
var1=$1
#boole b1 must be set if argument $1 exists (!-ne)
var2=$2
#boole b2
var3=$3
#boole b3
var4=$4
#boole b4
var5=$5
#boole b5
#now we have all we need

#todo toodoo check the five boolean
    if [ [ -z $var1 ] || [ -z $var2 ] || [ -z $var3 ] || [ -z $var4 ] || [ -z $var5 ] ]
    then
#whether exists or not output status and argument 
            if [ [!-z $var1] ]
            then
            echo argument ${var1} exists
#other case
            else
            echo argument ${var1} does not exist
            fi
            if [ [!-z $var2] ]
            then
            echo argument ${var2} exists
            else
            echo argument ${var2} does not exist
            fi
	        if [ [!-z $var3] ]
            then
            echo argument ${var3} exists
            else
            echo argument ${var3} does not exist
            fi
            if [ [!-z $var4] ]
            then
            echo argument ${var4} exists
            else
            echo argument ${var4} does not exist
            fi
            if [ [!-z $var5] ]
            then
            echo argument ${var5} exists
            else
            echo argument ${var5} does not exist
            fi
    else
#error message if invalid arguments
    echo "Arguments are zero or > 5"
#exit
    exit 1
    fi
#toodoo count length of array
printf 'slength:%s\n' "$slength";
